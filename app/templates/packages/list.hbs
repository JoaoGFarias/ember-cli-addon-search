{{em-header
  packageCount=packageCount
  query=query
  reset-page=(action 'resetPage')
}}

<div class="container">
  <div class="row">
    <div class="col-sm-12">
       
      {{em-list
        query=query
        list=model
        page=page
        limit=limit
        previous=(action 'previousPage')
        next=(action 'nextPage')
      }}

    </div>
  </div>
</div>
