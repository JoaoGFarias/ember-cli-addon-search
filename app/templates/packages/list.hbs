{{em-header
  packageCount=packageCount
  query=query
  reset-page=(action 'resetPage')
}}

<div class="container">
  <div class="row">
    <div class="col-sm-12">

      {{em-list
        list=model

        meta=(hash 
          query=query
          page=page
          limit=limit
          packageCount=packageCount
        )

        previous=(action 'previousPage')
        next=(action 'nextPage')
      }}

    </div>
  </div>
</div>
